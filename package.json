{
  "name": "signalk-vcan-logplayer",
  "version": "1.3.2",
  "description": "Replay / reprocess NMEA2000 log files with different format ('SignalK', 'Actisense', 'SocketCAN / candump', 'candump variant with timestamp')",
  "keywords": [
    "signalk-node-server-plugin",
    "signalk-category-nmea-2000",
    "signalk-category-utility",
    "signalk-plugin-configurator"
  ],
  "signalk-plugin-enabled-by-default": true,
  "main": "index.js",
  "author": "Dirk Behrendt",
  "license": "MIT",
  "dependencies": {
    "@canboat/canboatjs": "^3.11.0",
    "@signalk/n2k-signalk": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@signalk/server-admin-ui-dependencies": "^1.0.0",
    "@signalk/signalk-schema": "^1.7.2",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^5.1.4"
  },
  "signalk": {
    "plugin": {
      "type": "provider",
      "id": "vcan-logplayer",
      "name": "VCAN Log Player",
      "description": "Replay / reprocess NMEA2000 log files with different format ('SignalK', 'Actisense', 'SocketCAN / candump', 'candump variant with timestamp')"
    }
  },
  "files": [
    "index.js",
    "package.json",
    "README.md",
    "pgns/*.js",
    "utils.js",
    "public/",
    "src/",
    "webpack.config.js",
    ".babelrc"
  ],
  "scripts": {
    "build": "webpack --mode development"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": "https://github.com/formifan2002/signalk-vcan-logplayer",
  "bugs": {
    "url": "https://github.com/formifan2002/signalk-vcan-logplayer/issues"
  }
}
